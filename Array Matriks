#include <iostream>
using namespace std;

int main (){
	int x,y,total=0;
	
	cout<<"Masukkan jumlah lariknya :";
	cin>>x;
	cin>>y;
	int a[x][y];
	cout<<endl;
	
	for(int i=0;i<x;i++){
		for(int j=0;j<y;j++){
			cin>>a[i][j];
			if(j%2!=0){
				total=total+a[i][j];
			}
		}
	}
	cout<<total;
return 0;
}
